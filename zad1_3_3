#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c;
    printf("Podaj a: \n");
    scanf("%d", &a);
    printf("Podaj b: \n");
    scanf("%d", &b);
    printf("Podaj c: \n");
    scanf("%d", &c);
    printf("%d, %d, %d\n", a,b,c);
    if(a>b && a>c){
        printf("%d jest najwieksza liczba\n", a);
    }
    else if(b>a && b>c){
        printf("%d jest najwieksza liczba\n", b);
    }
    else if(c>a && c>b){
        printf("%d jest najwieksza liczba\n", c);
    }
    else if(a==b && b==c){
        printf("Wszystkie liczby sa takie same");
    }
    else{
        printf("blad");
    }


    return 0;
}
