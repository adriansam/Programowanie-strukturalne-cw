#include <stdio.h>
#include <stdlib.h>
int main()
{
    char lista[26];
    char znak;
    printf("Podaj napis o dlugosci 26 znakow: ");
    for(int i = 0; i<26; i++){
        scanf(" %c", &znak);
        lista[i] = znak;
    }
    int i = 0;
    while(lista[i]!='\0'){
        printf("%c", lista[i]);
        i++;
    }
    return 0;
}
